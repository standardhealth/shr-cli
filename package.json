  "repository": {
    "type": "git",
    "url": "git@github.com:standardhealth/shr-cli.git"
  },
  "main": "app.js",
  "scripts": {
    "ig:publish": "java -Xms4g -Xmx8g -jar ./out/fhir/guide/org.hl7.fhir.igpublisher.jar -ig ./out/fhir/guide/ig.json",
